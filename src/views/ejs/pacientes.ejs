<%- include ('partials/head') %>
<body>
    <h1>LISTA PACIENTES</h1>
    <table class="tabla-pacientes">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% paciente.forEach(pacientes=> { %>
                <tr>
                    <td>
                        <%= pacientes.nombre %>
                    </td>
                    <td>
                        <%= pacientes.apellido %>
                    </td>
                    <td>
                        <%= pacientes.email %>
                    </td>
                </tr>
                <% }); %>
        </tbody>
    </table>
</body>

</html>